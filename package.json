{
  "name": "@tickh/create-ec",
  "type": "module",
  "version": "0.1.25",
  "private": false,
  "packageManager": "pnpm@7.3.0",
  "description": "",
  "keywords": [],
  "homepage": " https://github.com/TickHeart/ec#readme",
  "bugs": {
    "url": " https://github.com/TickHeart/ec/issues"
  },
  "bin": {
    "create-ec": "./dist/index.js"
  },
  "license": "MIT",
  "author": "tick",
  "repository": {
    "type": "git",
    "url": "git+ https://github.com/TickHeart/ec"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish --no-git-checks",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@developer-plus/eslint-config": "^0.1.1",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "@types/shelljs": "^0.8.11",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "pnpm": "^7.3.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.8.8",
    "vitest": "^0.15.2"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "dayjs": "^1.11.3",
    "execa": "^6.1.0",
    "inquirer": "^9.0.0",
    "prettier": "^2.7.1",
    "shelljs": "^0.8.5",
    "ztshared": "^0.0.12"
  }
